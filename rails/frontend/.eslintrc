{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential","eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["vue"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}